<p id="notice"><%= notice %></p>

<h1>Realizados</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Fecha ini</th>
      <th>Fecha fina</th>
      <th>Tiempo</th>
      <th>Tarea</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @realizados.each do |realizado| %>
      <tr>
        <td><%= realizado.descripcion %></td>
        <td><%= realizado.fecha_ini %></td>
        <td><%= realizado.fecha_fina %></td>
        <td><%= realizado.tiempo %></td>
        <td><%= realizado.tarea_id %></td>
        <td><%= link_to 'Show', realizado %></td>
        <td><%= link_to 'Edit', edit_realizado_path(realizado) %></td>
        <td><%= link_to 'Destroy', realizado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Realizado', new_realizado_path %>
